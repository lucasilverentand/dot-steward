{
  "name": "@dot-steward/cli",
  "version": "0.0.0",
  "type": "module",
  "private": false,
  "description": "Dot Steward command line interface",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
